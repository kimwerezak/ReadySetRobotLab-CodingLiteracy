D\")\r\n\r\n#################################################################\r\n\r\n#Loops1, Finite Loop\r\nodometer = 0\r\nfor i in range (0, 1000000):\r\n    odometer = odometer + 1\r\n    basic.show_number(odometer) \r\n\r\n##################################################################\r\n\r\n#Loops2, Forever Loop\r\nspeed = 0\r\nwhile True:\r\n    if input.button_is_pressed(Button.A):\r\n        speed += 1\r\n        basic.show_number(speed)\r\n    \r\n    if input.button_is_pressed(Button.B):\r\n        speed-= 1\r\n        if speed < 0:\r\n            speed = 0\r\n        basic.show_number(speed)\r\n\r\n###################################################################\r\n\r\n#Loops3, Conditional Loop\r\nspeed = 0\r\ntank = 10\r\n\r\nwhile (tank>0):\r\n    if input.button_is_pressed(Button.A):\r\n        speed += 1\r\n        tank -= 1\r\n        basic.show_number(speed)\r\n    if input.button_is_pressed(Button.B):\r\n        speed -= 1\r\n        if speed<0:\r\n            speed = 0\r\n        basic.show_number(speed)\r\nbasic.show_string(\"Oh no, your tank is empty!\")\r\n\r\n###################################################################\r\n\r\n#Functions\r\ndef head_lights():\r\n    if input.light_level()<100:\r\n        basic.show_leds(\"\"\"\r\n            .....\r\n            .....\r\n            ##.##\r\n            #...#\r\n            .....\r\n            \"\"\")\r\n    else:\r\n        basic.clear_screen()\r\n\r\nwhile True:\r\n    head_lights()\r\n\r\n###################################################################\r\n\r\n\r\n#Events\r\ndef HL_on():\r\n    basic.show_leds(\"\"\"\r\n                .....\r\n                .....\r\n                ##.##\r\n                #...#\r\n                .....\r\n                \"\"\")\r\ndef HL_off():\r\n    basic.clear_screen()\r\n\r\ninput.on_logo_event(TouchButtonEvent.PRESSED, HL_on)\r\ninput.on_logo_event(TouchButtonEvent.LONG_PRESSED, HL_off)","pxt.json":"{\n    \"name\": \"RSRL\",\n   